  "dependencies": {
    "Dapper": {
      "version": "1.50-*",
      "target": "project"
    }
  },
  "commands": {
    "run": "Dapper.DNX.Tests",
    "test": "xunit.runner.dnx"
  },
  "compilationOptions": {
    "warningsAsErrors": true
  },
