        "System.Xml": "4.0.0.0"
      },
      "dependencies": {
        "xunit": "2.1.0"
      }
    },
    "dotnet5.4": {
      "compilationOptions": {
        "define": [ "ASYNC", "COREFX" ]
      },
      "dependencies": {
        "Microsoft.CSharp": "4.0.1-*",
        "System.Runtime": "4.0.21-*",
        "System.Collections": "4.0.11-*",
        "System.Console": "4.0.0-*",
        "System.Data.SqlClient": "4.0.0-*",
        "System.Linq": "4.0.1-*",
        "System.Threading": "4.0.11-*",
        "xunit": "2.1.0"
      }
    },
    "dnx451": {
      "compilationOptions": {
        "define": [ "ASYNC" ]
      },
      "dependencies": {
        "xunit": "2.1.0",
        "xunit.runner.dnx": "2.1.0-*"
      }
    },
    "dnxcore50": {
      "compilationOptions": {
        "define": [ "COREFX", "ASYNC" ]
      },
      "dependencies": {
        "xunit": "2.1.0",
        "xunit.runner.dnx": "2.1.0-*"
      }
    }
  }
}