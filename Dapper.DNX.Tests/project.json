    "net40": {
      "frameworkAssemblies": {
        "System.Data": "4.0.0.0",
        "System.Xml": "4.0.0.0"
      },
      "dependencies": {
        "xunit": "1.9.2"
      }
    },
    "net45": {
      "compilationOptions": {
        "define": [ "ASYNC" ]
      },
